    "@types/jest": "^27.4.0",
    "@types/uuid": "^8.3.4",
    "babel-jest": "^27.4.6",
    "jest": "^27.4.7",
    "prettier": "2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  }
}
